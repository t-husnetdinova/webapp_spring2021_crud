<h2 class="center">User Table</h2>
<table class="table">
    <thread>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thread>
    <tbody>
        <% users.forEach(subscriber => { %>
            <tr>
                <td>
                    <a href="<%= `/users/$user._id` %>">
                        <%= user.name %>
                    </a>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.zipCode %>
                </td>
                <td>
                    <a href="<%= `/users/${user._id}/edit` %>">
                        Edit
                    </a>
                </td>
                <td>
                    <a href="<%= `/users/${user._id}delete?_method=DELETE` %>" onClick="return confirm('Are you sure?')">
                        Delete
                    </a>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>